<%= search_form_for @q, url: admin_statistics_path do |f| %>
  <%= f.label "Take range from:" %><br>
  <%= f.date_select :updated_at_gteq %><br>
  <%= f.label "To:" %><br>
  <%= f.date_select :updated_at_lteq %><br>
  <br>
  <%= f.submit "Search", class: "btn btn-primary" %>
<% end %>
<%= link_to "Clear Search", admin_statistics_path, class: "cancel-button" %>
<br>
