<h2>Editing Category</h2>

<%= simple_form_for @category, as: :category, url: admin_category_path(@category) do |f| %>

  <%= render partial: 'form_fields', locals: { f: f } %>

  <%= f.button :submit, class: "btn btn-secondary" %>
  <%= link_to 'Back', admin_categories_path, class: "btn btn-secondary" %>
<% end %>